<div class="profile">
	<h3>My Profile</h3>
	<div>
		<h4>Name</h4>
		<p>{{firstName}} {{lastName}}</p>
		<h4>Username</h4>
		<p>{{username}}</p>
		<a href="/create-tutorial"><button>CREATE A TUTORIAL</button></a>
	</div>
</div>
<div class="container">
	<h3>My Tutorials</h3>
	{{#each tutorials}}
		<div id="{{_id}}" class="courses">
			<a href="/details/{{id}}">
				<img src="{{imageURL}}" />
				<h4>{{title}}</h4>
			</a>
			<div class="container">
				<a href="/delete-tutorial/{{id}}"><button
						id="delete"
					>DELETE</button></a>
				<a href="/edit-tutorial/{{id}}"><button
						id="edit"
					>EDIT</button></a>
			</div>
		</div>
	{{else}}
		<h2>No Tutorials...</h2>
	{{/each}}
</div>